<!doctype html>
<html lang="en">
<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-eOJMYsd53ii+scO/bJGFsiCZc+5NDVN2yr8+0RDqr0Ql0h+rP48ckxlpbzKgwra6" crossorigin="anonymous">
  <link rel="stylesheet" href="/styles/pages/login.css">
  <link rel="stylesheet" href="/styles/partials/buttons.css">
  <title>JobsCalc Login</title>


</head>
<body>
<div class="container d-flex justify-content-center">
  <div class="row">
    <div class="col-md">
      <img src="/images/img-login.JPG" class="mx-auto d-block" alt="logo">
    </div>
    <div class="col-md pt-5">
      <img src="/images/logo-login.jpg" height="84" width="280"/>
      <p class="h5 text-center">Efetue seu login</p>
      <input type="email" class="form-control mt-3" id="exampleFormControlInput1" placeholder="e-mail">
      <input type="password" class="form-control mt-3" id="exampleFormControlInput1" placeholder="senha">
      <div class="d-grid gap-2">
        <a href="/dash" target="_self" class="btn mt-3" id="btn1" type="button">Entrar</a>
        <a href="/register" target="_self" class="btn" id="btn2" type="button">Criar conta</a>
        <div class="d-flex justify-content-center links">
          <a href="#">Esqueceu sua senha?</a>
        </div>
      </div>
    </div>
  </div>
</div>

<!-- The core Firebase JS SDK is always required and must be listed first -->
